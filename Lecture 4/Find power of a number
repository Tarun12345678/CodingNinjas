import java.util.Scanner;
public class Solution {
    
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int a = s.nextInt();
        int b = s.nextInt();
        int i = 1;
        int sum = 1;
        while(i<=b){
            sum = sum*a;
            i++;
        }
        System.out.println(sum);
        
    }
}
