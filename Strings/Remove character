public class Solution {

	public static String removeAllOccurrencesOfChar(String str, char ch) {
	String ny = "";
        for(int i = 0;i<str.length();i++){
            if(str.charAt(i)==ch){
               continue;
                
            }
            else{
                ny +=str.charAt(i); 
               
            }
            
        }
        return ny;
      

        
        
        
	}

}
