import java.util.Scanner;
public class runner {

	public static void main(String[] args) {
				Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        for(int i=1;i<=n;i++){
            for(int j=n;j>=i+1;j--){
                System.out.print(j);
            }
            System.out.print("*");
            for(int k = 1;k<i;k++){
                System.out.print(i-k);
            }
            System.out.println();
            
        }

	}
}
