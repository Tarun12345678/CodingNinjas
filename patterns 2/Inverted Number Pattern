import java.util.Scanner;
public class Solution {


	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        int i = 1;
        int k = n;
        while(i<=n){
            int j = 1;
            while(j<=n-i+1){
                System.out.print(k);
                j++;
            }
            System.out.println();
            k--;
            i++;
        }


		
	}

}
