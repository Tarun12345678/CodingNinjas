import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
    Scanner s = new Scanner (System.in);
    int n = s.nextInt();
    int k = 1;
      
    for (int i = 1; i <n; i++)
         
      {
	if (i*i> n)
	  {
	    k = i - 1;
	    break;

	  }
    }
        if(n==0){
            System.out.print(0);
            return;
        }













      
      System.out.print(k);
        
		

	}
}
