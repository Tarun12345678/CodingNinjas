import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
	    Scanner s = new Scanner(System.in);
	    int n = s.nextInt();
	    int d = 4;
	    
	    for(int i=1;i<=n;i++){
	        int x = 3*i+2;
	        if(x%d!=0){
	            System.out.print(x+" ");
	            
	        }
	        else{
	            n=n+1;
	        }

	        }
		

	}
}
