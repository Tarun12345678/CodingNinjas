import java.util.Scanner;
public class Solution {


	public static void main(String[] args) {
	    Scanner s = new Scanner(System.in);
	    int n = s.nextInt();
	   int k=0;
	   int y=1;
	   int j = 0;
	   int h =0;
	    for(int i = 1;i<n;i++){
	      j = k+y;
	      
	      k=y;
	      y=j;
	      
	    }
	    System.out.print(y);


		
	}

}
